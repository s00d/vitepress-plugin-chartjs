{
  "name": "vitepress-plugin-chartjs",
  "version": "1.1.2",
  "description": "Chart.js plugin for VitePress - render charts from markdown code blocks",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/vitepress-plugin-chartjs.umd.js",
  "module": "./dist/vitepress-plugin-chartjs.es.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vitepress-plugin-chartjs.es.mjs",
      "require": "./dist/vitepress-plugin-chartjs.umd.js"
    },
    "./VpChart.vue": "./dist/VpChart.vue"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "pnpm build && pnpm -C example docs:dev",
    "docs:build": "pnpm build && pnpm -C example docs:build",
    "docs:preview": "pnpm -C example docs:preview"
  },
  "dependencies": {
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "chart.js": "^4.5.0",
    "typescript": "^5.9.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-static-copy": "^3.0.0",
    "vitepress": "^1.0.0",
    "vue": "^3.5.0"
  },
  "peerDependencies": {
    "chart.js": "^4.0.0",
    "vitepress": "^1.0.0"
  },
  "keywords": [
    "vitepress",
    "chartjs",
    "chart.js",
    "charts",
    "markdown",
    "visualization"
  ],
  "author": "s00d",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/s00d/vitepress-plugin-chartjs.git"
  },
  "homepage": "https://github.com/s00d/vitepress-plugin-chartjs",
  "bugs": {
    "url": "https://github.com/s00d/vitepress-plugin-chartjs/issues"
  }
}
